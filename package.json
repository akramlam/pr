{
  "name": "smarter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run server\"",
    "server": "cross-env NODE_ENV=development nodemon --watch \"server/**/*.ts\" --exec \"tsx\" ./server/index.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production tsx ./server/index.ts",
